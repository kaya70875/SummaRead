{
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "permissions" : ["activeTab"],
    "action": {
      "default_popup": "main.html"
    },
    "content_scripts": [
        {
            "matches" : ["<all_urls>"],
            "js" : ["scripts/fetchContent.js", "node_modules/mark.js/dist/mark.min.js"],
            "css" : ["styles/inject.css"],
            "type" : "module"
        }
    ]
  }